<div class="navbar">
  <div class="_left">
    <a href="https://winnetoujs.org">
      <img src="/images/winnetoujs-official-logo-2024-framework-javascript-web-development.png" />
      <span class="_title">WinnetouJs</span>
      <span class="_version">LTS</span>
      <script>
        (async () => {
          let data = await fetch("/npm/version");
          let version = await data.json();
          document.querySelector("._version").innerHTML = version;
        })();
      </script>
      <span class="_version">
        <% var t='' ; if(type=='docs' ){ t='Documentation' ; } else { t="API Reference" ; } %>
        <%= t %>
      </span>
    </a>
  </div>
  <div class="_right">
    <a class="" href="/docs">Docs</a>
    <a class="" href="/api">API</a>

    <a class="" href="/docs/change-log">Change Log</a>

    <a class="" href="https://github.com/cedrosdev/winnetoujs" target="_blank">
      GitHub
    </a>
    <a class="" href="/docs/contributing">Pull Requests Guideline</a>
    <a class="" href="/docs/get-in-touch">Get in touch</a>
  </div>
  <div class="_menu">
    <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512" id="_bars">
