  </div>
</div>
<div class="rainbow"></div>

<script>
  let menuVisible = false;
  document.querySelector("._menu").addEventListener("click", ev => {
    if (!menuVisible) {
      menuVisible = true;
      document.querySelector(".leftMenu").style.display = "block";
      document.querySelector("#_bars").style.display = "none";
      document.querySelector("#_x").style.display = "block";
    } else {
      menuVisible = false;
      document.querySelector(".leftMenu").style.display = "none";
      document.querySelector("#_bars").style.display = "block";
      document.querySelector("#_x").style.display = "none";
    }
  });
</script>